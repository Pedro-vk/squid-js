{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "max-line-length": [true, 140],
    "semicolon": [
      true,
      "never"
    ],
    "no-empty": [true, "allow-empty-catch", "allow-empty-functions"],
    "ordered-imports": false
  },
  "rulesDirectory": []
}
